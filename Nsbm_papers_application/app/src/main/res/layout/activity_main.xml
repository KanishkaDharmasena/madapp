<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dd"
    >

    <Button
        android:id="@+id/button4"
        android:layout_width="82dp"
        android:layout_height="57dp"
        android:layout_marginTop="72dp"
        android:background="#6C756D"
        android:text="Nlearn"
        android:textColor="#FFC107"
        android:textSize="10sp"
        android:textStyle="bold"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="389dp"
        android:layout_height="152dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="44dp"
        android:text="Choose Your "
        android:textColor="#550511"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text2"
        android:layout_width="271dp"
        android:layout_height="93dp"
        android:layout_marginTop="88dp"
        android:text="Faculty  "
        android:textColor="#420923"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.721"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="208dp"
        android:layout_height="62dp"
        android:layout_marginTop="68dp"
        android:background="#0D8C12"
        android:text="Computing"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="209dp"
        android:layout_height="68dp"
        android:layout_marginTop="40dp"
        android:background="#0D8C12"
        android:text="Management"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button3"
        android:layout_width="211dp"
        android:layout_height="65dp"
        android:layout_marginTop="36dp"
        android:background="#0D8C12"
        android:text="Engineering"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        tools:ignore="MissingConstraints" />

    <Button
        android:background="#7E8A7F"
        app:backgroundTint="@null"
        android:id="@+id/button5"
        android:layout_width="82dp"
        android:textColor="#FFC107"
        android:layout_height="57dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="84dp"
        android:text="Lecturer "
        android:textSize="8sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/button20"
        app:layout_constraintHorizontal_bias="0.995"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button6"
        android:layout_width="79dp"
        android:layout_height="57dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="28dp"
        android:text="Email"
        android:textSize="10dp"
        android:textStyle="bold"
        android:background="#5C5352"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:ignore="MissingConstraints"
        android:textColor="#FFC107"/>

    <Button
        android:id="@+id/button20"
        android:layout_width="87dp"
        android:layout_height="60dp"
        android:layout_marginTop="72dp"
        android:background="#9FA39F"
        android:text="Profile"
        android:textColor="#FFC107"
        android:textSize="10sp"
        android:textStyle="bold"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button7"
        android:layout_width="85dp"
        android:layout_height="58dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="60dp"
        android:text="NSBM Club"
        android:textSize="10dp"
        android:textStyle="bold"
        android:background="#3E3E3E"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        android:textColor="#FFC107"
        />

    <ImageView
        android:id="@+id/im12"
        android:layout_width="63dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logout123" />

</androidx.constraintlayout.widget.ConstraintLayout>
